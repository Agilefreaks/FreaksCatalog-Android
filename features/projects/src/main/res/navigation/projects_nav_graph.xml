<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/projects_nav_graph"
    app:startDestination="@id/projects"
    tools:ignore="UnusedIds,UnusedResources">
    <fragment
        android:id="@+id/projects"
        android:name="com.agilefreaks.freaks_catalog.features.projects.ProjectsFragment"
        android:label="@string/projects_title">
        <action
            android:id="@+id/action_projects_to_projectDetailsFragment"
            app:destination="@id/projectDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/projectDetailsFragment"
        android:name="com.agilefreaks.freaks_catalog.features.projects.details.ProjectDetailsFragment"
        android:label="ProjectDetailsFragment" >
        <argument
            android:name="projectId"
            app:argType="string"/>
    </fragment>
</navigation>
